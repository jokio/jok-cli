	"dependencies": {
		"chalk": "2.4.1",
		"commander": "2.19.0",
		"jokio": "0.3.0"
	},
	"devDependencies": {
		"@types/chalk": "2.2.0",
		"@types/commander": "2.12.2",
		"@types/jest": "23.3.9",
		"dotenv": "6.0.0",
		"fs-extra": "7.0.1",
		"jest": "23.6.0",
		"nodemon": "1.14.11",
		"ts-jest": "23.10.4",
		"ts-node": "4.0.1",
		"tslint": "5.11.0",
		"typescript": "3.0.1"
	},
	"repository": {
		"type": "git",
		"url": ""
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"jok",
		"cli"
	],
	"license": "MIT",
	"jest": {
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testURL": "http://localhost",
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
