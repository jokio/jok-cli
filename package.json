{
	"name": "jok",
	"version": "0.1.0",
	"author": "",
	"description": "",
	"main": "dist/index.js",
	"types": "dist/index.d.js",
	"bin": "./dist/program.js",
	"scripts": {
		"start": "ts-node src",
		"test": "ts-node ./node_modules/jokio/src/_test",
		"dev": "nodemon",
		"build": "tsc",
		"install-locally": "npm run build && npm pack && sudo npm i -g jok-0.1.0.tgz && rm -f jok-0.1.0.tgz && rm -fr dist/"
